@model IEnumerable<Uppgift4Interaktiva.Models.TvProgram>
@{
    ViewBag.Title = "Index";
}

<div class="jumbotron">
    <h1>TV-KOLLEN</h1>
    <p class="lead">Tv-kollen är internets bästa guide för vilka TVprogram som går!</p>
</div>

<div class="row">
    <div class="col-md-3">
        <h2>SVT 1</h2>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Start)
                </th>
       
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Start)
                    </td>
              
                </tr>
            }

        </table> 
   
   
</div>
